{
  "name": "medusa-plugin-invoiceahoy",
  "version": "0.1.0",
  "description": "A Medusa Commerce plugin to create invoices with InvoiceAhoy.",
  "author": "InvoiceAhoy (https://invoiceahoy.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/invoiceahoy/medusa-plugin-invoiceahoy"
  },
  "license": "MIT",
  "keywords": [
    "medusa-plugin",
    "medusa-plugin-notification",
    "medusa",
    "invoice",
    "invoiceahoy",
    "ecommerce"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env yarn clean && yarn build:server",
    "build:server": "cross-env yarn clean && tsc -p tsconfig.server.json",
    "build:admin": "cross-env medusa-admin build",
    "watch": "cross-env tsc --watch",
    "test": "jest --passWithNoTests",
    "prepare": "cross-env NODE_ENV=production yarn run build"
  },
  "dependencies": {
    "@medusajs/cache-inmemory": "^1.8.9",
    "@medusajs/cache-redis": "^1.8.9",
    "@medusajs/event-bus-local": "^1.9.7",
    "@medusajs/event-bus-redis": "^1.8.10",
    "babel-preset-medusa-package": "^1.1.13",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "express": "^4.17.2",
    "invoiceahoy": "^1.1.8",
    "medusa-core-utils": "^1.2.0",
    "medusa-interfaces": "^1.3.7",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/medusa": "1.17.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@medusajs/medusa": "1.17.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
